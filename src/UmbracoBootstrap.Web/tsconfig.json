{
  "compileOnSave": true,
  "compilerOptions": {
    "lib": [ "dom", "es2015" ],
    "target": "es5",
    "module": "commonjs",
    "sourceMap": true,
    "removeComments": true,
    "suppressImplicitAnyIndexErrors": true,
    "preserveConstEnums": true,
    "alwaysStrict": true,
    "jsx": "react"
  },
  "exclude": [
    "node_modules"
  ]
}
